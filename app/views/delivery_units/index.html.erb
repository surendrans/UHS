<h1>Listing delivery_units</h1>

<table>
  <tr>
    <th>Manufacturer</th>
    <th>Unit model</th>
    <th>Desc</th>
    <th>Prefix serial</th>
    <th>Delivery</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @delivery_units.each do |delivery_unit| %>
  <tr>
    <td><%= delivery_unit.manufacturer %></td>
    <td><%= delivery_unit.unit_model %></td>
    <td><%= delivery_unit.desc %></td>
    <td><%= delivery_unit.prefix_serial %></td>
    <td><%= delivery_unit.delivery %></td>
    <td><%= link_to 'Show', delivery_unit %></td>
    <td><%= link_to 'Edit', edit_delivery_unit_path(delivery_unit) %></td>
    <td><%= link_to 'Destroy', delivery_unit, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

